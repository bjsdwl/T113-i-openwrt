CONFIG_TARGET_sunxi=y
CONFIG_TARGET_sunxi_cortexa7=y
# 必须显式选择我们的新机型
CONFIG_TARGET_sunxi_cortexa7_DEVICE_tronlong_tlt113-evm=y

# 核心驱动
CONFIG_PACKAGE_kmod-motorcomm=y
CONFIG_PACKAGE_kmod-usb-net-sr9900=y
CONFIG_PACKAGE_kmod-rtl8188eu=y

# 文件系统支持 (可选)
CONFIG_PACKAGE_kmod-fs-ext4=y
CONFIG_PACKAGE_kmod-fs-vfat=y
